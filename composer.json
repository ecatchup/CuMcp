{
  "name": "ecatchup/cu-mcp",
  "description": "CuMcp plugin for baserCMS",
  "type": "cakephp-plugin",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "ext-openssl": "*",
    "league/oauth2-server": "^8.5",
    "php-mcp/server": "^3.3",
    "nyholm/psr7": "^1.8",
    "symfony/psr-http-message-bridge": "^2.3"
  },
  "autoload": {
    "psr-4": {
      "CuMcp\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CuMcp\\Test\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "cakephp": {
      "plugin-name": "CuMcp"
    }
  }
}
