{
    "name": "ecatchup/cu-mcp",
    "description": "CuMcp plugin for baserCMS",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-openssl": "*",
        "league/oauth2-server": "^8.5",
        "php-mcp/server": "^3.3",
        "nyholm/psr7": "^1.8",
        "symfony/psr-http-message-bridge": "^2.3",
        "baserproject/baser-core": "^5.1.10"
    },
    "require-dev": {
        "phpunit/phpunit": "10.5.31",
        "josegonzalez/dotenv": "^4.0",
        "vierge-noire/cakephp-fixture-factories": "^3.0",
        "vierge-noire/cakephp-test-suite-light": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "CuMcp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CuMcp\\Test\\": "tests/",
            "App\\": "tests/TestApp/src/",
            "BaserCore\\Test\\": "vendor/baserproject/baser-core/tests/",
            "BcBlog\\Test\\": "vendor/baserproject/bc-blog/tests/",
            "BcSearchIndex\\Test\\": "vendor/baserproject/bc-search-index/tests/",
            "BcCustomContent\\Test\\": "vendor/baserproject/bc-custom-content/tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "cakephp/plugin-installer": true
        }
    },
    "extra": {
        "cakephp": {
            "plugin-name": "CuMcp"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
