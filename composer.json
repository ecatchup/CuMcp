{
    "name": "your-name-here/cu-mcp",
    "description": "CuMcp plugin for baserCMS",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": "5.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.3"
    },
    "autoload": {
        "psr-4": {
            "CuMcp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CuMcp\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
